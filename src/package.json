{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "@systemic/select-guru",
  "version": "0.0.5",
  "description": "Angular ng-select - All in One UI Select, Multiselect and Autocomplete",
  "author": "prabakaran",
  "license": "MIT",
  "repository": "git@github.com:vscodeguru/select-guru.git",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "angular",
    "select",
    "ui-select",
    "dropdown",
    "multiselect",
    "autocomplete",
    "angular2",
    "angular4"
  ],
  "peerDependencies": {
    "@angular/common": ">=5.0.0 <7.0.0",
    "@angular/core": ">=5.0.0 <7.0.0",
    "@angular/forms": ">=5.0.0 <7.0.0"
  },
  "ngPackage": {
    "lib": {
      "flatModuleFile": "select-guru",
      "entryFile": "index.ts"
    },
    "dest": "../dist"
  },
  "readme": "README.md"
}
